<Application x:Class="GameTranslaterUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GameTranslaterUI"
             StartupUri="WindowLogin.xaml" Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="StyleDictionary_Button.xaml" />
                <ResourceDictionary Source="StyleDictionary_CheckBox.xaml" />
                <ResourceDictionary Source="StyleDictionary_Radio.xaml" />
                <ResourceDictionary Source="StyleDictionary_Slider.xaml" />
                <ResourceDictionary Source="StyleDictionary_TextBox.xaml" />
                <ResourceDictionary Source="StyleDictionary_Tab.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
